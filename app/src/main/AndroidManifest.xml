<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jrubio.zombietap">


    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_game"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_game_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Zombie_Tap"
        tools:targetApi="31">
        <activity
            android:name="com.jrubio.zombie_tap.ui.theme.Puntajes"
            android:exported="false" />
        <activity
            android:name="com.jrubio.zombie_tap.ui.theme.escenarioJuego"
            android:exported="false" />
        <activity
            android:name="com.jrubio.zombie_tap.ui.theme.Login"
            android:exported="false" />
        <activity
            android:name="com.jrubio.zombie_tap.ui.theme.Menu"
            android:exported="false" />
        <activity
            android:name="com.jrubio.zombie_tap.ui.theme.Registro"
            android:exported="false" />
        <activity
            android:name="com.jrubio.zombie_tap.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jrubio.zombie_tap.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>